        "drupal/workbench_moderation": "7.1.*",
        "drupal/wysiwyg": "7.2.*",
        "drupal/xautoload": "7.4.*"
  },
  "skip-due-to-issues": {
        "drupal/zurb-foundation": "7.4.*"
  },
  "removed-not-sure-if-needed": {
        "davidbarratt/custom-installer": "dev-master"
  },
  "ignore-but-document-recommendations": {
	"drupal/ctools": "suggests installing drupal/panels (Required by drupal/ctools_plugin_example)",
	"drupal/ctools": "suggests installing drupal/advanced_help (Required by drupal/ctools_plugin_example)",
	"drupal/date": "suggests installing drupal/migrate (Required by drupal/date_migrate_example)",
	"drupal/eck": "suggests installing drupal/services (Required by drupal/eck_services)",
	"drupal/media": "suggests installing drupal/multiform (Required by drupal/media_bulk_upload)",
	"drupal/media": "suggests installing drupal/plupload (Required by drupal/media_bulk_upload)",
	"drupal/features_extra": "suggests installing drupal/nodequeue (Required by drupal/fe_nodequeue)",
	"drupal/message": "suggests installing drupal/og (Required by drupal/message_og_example)",
	"drupal/rules": "suggests installing drupal/i18n_string (Required by drupal/rules_i18n)"
  },
  "sbac-local-modules": {
        "drupal/sbac_acquia": "7.1.*",
        "drupal/sbac_acquia_search": "7.1.*",
        "drupal/sbac_air": "7.1.*",
        "drupal/sbac_alerts": "7.1.*",
        "drupal/sbac_alignment": "7.1.*",
        "drupal/sbac_central": "7.1.*",
        "drupal/sbac_core": "7.1.*",
        "drupal/sbac_digital_library_resources": "7.1.*",
        "drupal/sbac_favorites": "7.1.*",
        "drupal/sbac_flag": "7.1.*",
        "drupal/sbac_forum": "7.1.*",
        "drupal/sbac_forum_search": "7.1.*",
        "drupal/sbac_help": "7.1.*",
        "drupal/sbac_landing_page": "7.0.*",
        "drupal/sbac_learning_registry": "7.1.*",
        "drupal/sbac_lexicon": "7.1.*",
        "drupal/sbac_media": "7.1.*",
        "drupal/sbac_report": "7.1.*",
        "drupal/sbac_resource": "7.1.*",
        "drupal/sbac_search": "7.1.*",
        "drupal/sbac_share": "7.1.*",
        "drupal/sbac_sso": "7.1.*",
        "drupal/sbac_user": "7.1.*",
        "drupal/sbac_workbench": "7.1.*"
  },
  "conflict": {
    "drupal/core": "8.*"
  },
  "scripts": {
    "post-create-project-cmd": [
      "rm README.md LICENSE .travis.yml phpunit.xml.dist"
    ]
  },
  "config": {
    "vendor-dir": "vendor",
    "disabled-vendor-dir": "docroot/sites/all/vendor"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "docroot/": ["type:drupal-core"],
      "docroot/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
      "docroot/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"],
      "docroot/sites/all/libraries/{$name}/": ["type:drupal-library"],
      "docroot/sites/all/drush/{$name}/": ["type:drupal-drush"],
      "docroot/profiles/{$name}/": ["type:drupal-profile"]
    },
    "disabled-custom-installer": {
      "drupal-module": "docroot/sites/all/modules/contrib/{$name}/",
      "drupal-theme": "docroot/sites/all/themes/contrib/{$name}/",
      "drupal-library": "docroot/sites/all/libraries/{$name}/",
      "drupal-drush": "docroot/sites/all/drush/{$name}/",
      "drupal-profile": "docroot/profiles/{$name}/",
      "drupal-core": "docroot/"
    },
    "preserve-paths": [
      "docroot/sites/all/modules/contrib",
      "docroot/sites/all/themes/contrib",
      "docroot/sites/all/libraries",
      "docroot/sites/all/drush",
      "docroot/profiles",
      "docroot/sites/default/settings.php",
      "docroot/sites/default/files"
    ]
  }
}
