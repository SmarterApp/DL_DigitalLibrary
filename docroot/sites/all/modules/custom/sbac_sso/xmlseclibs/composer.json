{
    "name": "fr3d/xmlseclibs",
    "description": "Unofficial git fork of http://code.google.com/p/xmlseclibs/",
    "license": "BSD-3-Clause",
    "keywords": [
        "xml",
        "xmldsig",
        "signature",
        "security"
    ],
    "homepage": "http://code.google.com/p/xmlseclibs/",
    "autoload": {
        "files": ["xmlseclibs.php"]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "cdatazone/xmlseclibs",
                "version": "dev-master",
                "source": {
                    "type": "svn",
                    "url": "http://xmlseclibs.googlecode.com/svn/",
                    "reference": "trunk/"
                },
                "autoload": {
                    "classmap": ["xmlseclibs.php"]
                }
            }
        }
    ],
    "provide": {
        "cdatazone/xmlseclibs": "1.3.0"
    },
    "suggest": {
        "ext/openssl": "OpenSSL extension",
        "ext/mcrypt": "MCrypt extension"
    }
}
